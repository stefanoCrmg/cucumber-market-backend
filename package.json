{
  "name": "cucuber-market-api",
  "version": "1.0.0",
  "main": "dist/main.js",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -- cross-var concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/main.js",
    "build": "swc src -d dist",
    "start": "NODE_ENV=production node dist/main.js",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "test": "jest",
    "prisma:migrate": "prisma migrate dev --name init"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.0",
    "@swc/jest": "^0.2.22",
    "@types/content-type": "^1.1.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.4.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-fp-ts": "^0.3.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "fast-check": "^3.1.3",
    "jest": "^29.0.3",
    "jest-mock-extended": "^3.0.0-beta1",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "supertest": "^6.2.4",
    "ts-toolbelt": "^9.6.0",
    "typescript": "*"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@unsplash/sum-types": "^0.3.0",
    "body-parser": "^1.20.0",
    "content-type": "^1.0.4",
    "cross-var": "^1.1.0",
    "dotenv": "^16.0.2",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.1",
    "fp-ts": "^2.12.3",
    "fp-ts-contrib": "^0.1.29",
    "fp-ts-routing": "^0.5.4",
    "fp-ts-std": "^0.15.0",
    "hyper-ts": "^0.7.8",
    "io-ts": "^2.2.18",
    "io-ts-reporters": "^2.0.1",
    "io-ts-types": "^0.5.16",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "picocolors": "^1.0.0",
    "prisma": "^4.3.1",
    "ts-adt": "^2.1.2",
    "ts-pattern": "^4.0.5",
    "undici": "^5.10.0"
  }
}
